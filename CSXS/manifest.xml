<?xml version="1.0" encoding="UTF-8"?>
<ExtensionManifest Version="12.0" ExtensionBundleId="com.orbitools.panel">
    <ExtensionList>
        <Extension Id="com.orbitools.panel" Version="1.0" />
    </ExtensionList>
    <ExecutionEnvironment>
        <HostList>
            <Host Name="AEFT" Version="[25.0,99.9]" />
        </HostList>
        <LocaleList>
            <Locale Code="All" />
        </LocaleList>
        <RequiredRuntimeList>
            <RequiredRuntime Name="CSXS" Version="12.0" />
        </RequiredRuntimeList>
    </ExecutionEnvironment>
    <DispatchInfoList>
        <Extension Id="com.orbitools.panel">
            <DispatchInfo>
                <Resources>
                    <MainPath>./host/index.html</MainPath>
                    <ScriptPath>./client/main.jsx</ScriptPath>
                </Resources>
                <Lifecycle>
                    <AutoVisible>true</AutoVisible>
                </Lifecycle>
                <UI>
                    <Type>Panel</Type>
                    <Menu>OrbitTools AE</Menu>
                    <Geometry>
                        <Size>
                            <Height>300</Height>
                            <Width>400</Width>
                        </Size>
                    </Geometry>
                </UI>
                <CEFCommandLine>
                    <Parameter>--enable-nodejs</Parameter>
                    <Parameter>--mixed-context</Parameter>
                </CEFCommandLine>
            </DispatchInfo>
        </Extension>
    </DispatchInfoList>
</ExtensionManifest>